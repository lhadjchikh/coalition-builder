{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.9.0",
    "polished": "^4.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "styled-components": "^6.1.19",
    "web-vitals": "^4.2.4",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "preview": "serve -s build",
    "test": "jest",
    "test:ci": "jest --verbose --coverage",
    "test:e2e": "jest src/tests/e2e --verbose",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:ci": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,json}\" \"../**/*.{md,yml,yaml,js,jsx,ts,tsx,css,json}\" --ignore-path ../.prettierignore",
    "yaml:format": "prettier --write \"../**/*.{yml,yaml}\" --ignore-path ../.prettierignore",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.26",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/styled-components": "^5.1.34",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
